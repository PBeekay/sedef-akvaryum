# Environment Variables Example
# Copy this file to .env.local for local development

# Admin Authentication
REACT_APP_ADMIN_USERNAME=admin
REACT_APP_ADMIN_PASSWORD=your_secure_password_here

# JWT Secret (Generate a strong secret for production)
REACT_APP_JWT_SECRET=your_jwt_secret_key_here

# Google Places API (Optional)
REACT_APP_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id_here

# Build Configuration
GENERATE_SOURCEMAP=false
NODE_ENV=production

# Security
REACT_APP_ENABLE_SECURITY_HEADERS=true
REACT_APP_RATE_LIMIT_ENABLED=true
